    <menuitem name="Geolocation" parent="menu_root" id="solesgps_map_geolocation" sequence="2" groups="gpsmap_group_user"/>
        
    <record id="action_gpsmap_maponline" model="ir.actions.client">
        <field name="name">Geolocation</field>
